<?xml version="1.0" encoding="utf-8"?>
<com.citywithincity.widget.TabHostView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" 
        android:id="@id/_container"
        android:layout_above="@id/_bottom_bar">
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_tabhost_bottom_height"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        android:id="@id/_bottom_bar" >

    </LinearLayout>

</com.citywithincity.widget.TabHostView>
